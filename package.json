    "config:copy": "cp -f -p -v ./config/config.xml ./ && cp -f -p -v ./config/build.json ./ && cp -R -f -p -v ./config/analytics ./ && cp -R -f -p -v ./config/resources ./ && cp -R -f -p -v ./config/src ./",
    "config:install": "npm run config:copy && npm install && npx jetifier && ionic cordova resources",
    "config:clean": "ionic cordova platform rm android && ionic cordova platform rm ios && rm -rf www && rm -rf node_modules && rm -rf plugins",
    "config:uninstall": "npm run config:clean && git reset --hard && rm -rf config && rm -rf analytics && rm build.json",
    "build:android": "ionic cordova build android --prod --release",
    "build:ios": "ionic cordova build ios --prod --release"
  },
