{
  "name": "geotrek-mobile",
  "version": "3.7.0",
  "author": "Makina Corpus",
  "homepage": "https://makina-corpus.com",
  "engines": {
    "node": ">=11.x"
  },
  "scripts": {
    "prettier": "prettier --write 'src/**/*.{ts,js,scss,html}'",
    "lint": "ng lint",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "compodoc": "npx compodoc -p src/tsconfig.app.json -d ./docs",
    "config:copy": "cp -f -p -v ./config/config.xml ./ && cp -f -p -v ./config/build.json ./ && cp -R -f -p -v ./config/analytics ./ && cp -R -f -p -v ./config/resources ./ && cp -R -f -p -v ./config/src ./",
    "config:install": "npm run config:copy && npm install && ionic cordova resources",
    "config:clean": "ionic cordova platform rm android && ionic cordova platform rm ios && rm -rf www && rm -rf node_modules && rm -rf plugins",
    "config:uninstall": "npm run config:clean && git reset --hard && rm -rf config && rm -rf analytics && rm build.json",
    "build:apk": "ionic cordova build android --prod --release",
    "build:ipa": "ionic cordova build ios --prod --release"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/common": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/http": "^7.2.16",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@ionic-native/background-geolocation": "^5.34.0",
    "@ionic-native/core": "^5.23.0",
    "@ionic-native/device-orientation": "^5.27.0",
    "@ionic-native/file": "^5.23.0",
    "@ionic-native/firebase-analytics": "^5.23.0",
    "@ionic-native/globalization": "^5.23.0",
    "@ionic-native/ionic-webview": "^5.23.0",
    "@ionic-native/local-notifications": "^5.23.0",
    "@ionic-native/network": "^5.23.0",
    "@ionic-native/screen-orientation": "^5.23.0",
    "@ionic-native/social-sharing": "^5.23.0",
    "@ionic-native/splash-screen": "^5.23.0",
    "@ionic-native/status-bar": "^5.23.0",
    "@ionic-native/zip": "^5.23.0",
    "@ionic/angular": "^4.11.10",
    "@ionic/storage": "^2.2.0",
    "@mauron85/cordova-plugin-background-geolocation": "^3.1.0",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@turf/distance": "^6.0.1",
    "@turf/helpers": "^6.1.4",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.deburr": "^4.1.6",
    "@types/mapbox-gl": "^1.8.1",
    "cordova-android-play-services-gradle-release": "3.0.0",
    "cordova-plugin-badge": "^0.8.8",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-device-orientation": "^2.0.1",
    "cordova-plugin-file": "^6.0.2",
    "cordova-plugin-firebase-analytics": "^2.0.4",
    "cordova-plugin-globalization": "1.11.0",
    "cordova-plugin-ionic-keyboard": "^2.2.0",
    "cordova-plugin-ionic-webview": "^4.1.3",
    "cordova-plugin-local-notification": "^0.9.0-beta.2",
    "cordova-plugin-network-information": "^2.0.2",
    "cordova-plugin-screen-orientation": "^3.0.2",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova-plugin-x-socialsharing": "^5.6.4",
    "cordova-plugin-zip": "3.1.0",
    "cordova-sqlite-storage": "^5.0.0",
    "cordova-support-android-plugin": "^1.0.1",
    "cordova-support-google-services": "^1.4.0",
    "core-js": "^3.6.4",
    "es6-promise-plugin": "4.2.2",
    "lodash.clonedeep": "^4.5.0",
    "lodash.deburr": "^4.1.0",
    "lodash.throttle": "^4.1.1",
    "mapbox-gl": "^1.10.0",
    "rxjs": "^6.5.4",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/architect": "^0.803.25",
    "@angular-devkit/build-angular": "^0.803.25",
    "@angular-devkit/core": "^8.3.25",
    "@angular-devkit/schematics": "^8.3.25",
    "@angular/cli": "^8.3.25",
    "@angular/compiler": "^8.2.14",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@compodoc/compodoc": "^1.1.11",
    "@ionic/angular-toolkit": "^2.2.0",
    "@types/node": "^12.12.30",
    "codelyzer": "^5.2.1",
    "cypress": "^7.4.0",
    "prettier": "^1.19.1",
    "ts-node": "^8.8.1",
    "tslint": "^5.20.1",
    "typescript": "~3.5.3"
  },
  "description": "A hiking app",
  "cordova": {
    "plugins": {
      "cordova-plugin-file": {},
      "cordova-plugin-zip": {},
      "cordova-sqlite-storage": {},
      "cordova-plugin-whitelist": {},
      "cordova-plugin-statusbar": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-ionic-keyboard": {},
      "cordova-plugin-globalization": {},
      "cordova-plugin-network-information": {},
      "cordova-plugin-x-socialsharing": {
        "ANDROID_SUPPORT_V4_VERSION": "24.1.1+"
      },
      "cordova-plugin-firebase-analytics": {
        "FIREBASE_CORE_VERSION": "16.0.+"
      },
      "cordova-support-google-services": {},
      "cordova-android-play-services-gradle-release": {
        "PLAY_SERVICES_VERSION": "16.+"
      },
      "cordova-plugin-ionic-webview": {
        "ANDROID_SUPPORT_ANNOTATIONS_VERSION": "27.+"
      },
      "cordova-plugin-screen-orientation": {},
      "cordova-plugin-local-notification": {},
      "cordova-plugin-background-geolocation": {
        "GOOGLE_PLAY_SERVICES_VERSION": "11+",
        "ANDROID_SUPPORT_LIBRARY_VERSION": "26+",
        "ICON": "@mipmap/icon",
        "SMALL_ICON": "@mipmap/icon",
        "ALWAYS_USAGE_DESCRIPTION": "In order to locate or track you while hiking, this app always requires location tracking",
        "MOTION_USAGE_DESCRIPTION": "In order to locate or track you while hiking, this app requires motion detection",
        "ACCOUNT_NAME": "@string/app_name",
        "ACCOUNT_LABEL": "@string/app_name",
        "ACCOUNT_TYPE": "$PACKAGE_NAME.account",
        "CONTENT_AUTHORITY": "$PACKAGE_NAME"
      },
      "cordova-plugin-device-orientation": {}
    },
    "platforms": []
  }
}
